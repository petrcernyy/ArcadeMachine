    out = 0;

    t=timer('ExecutionMode','fixedrate','Period', 5, 'TasksToExecute', 1, 'TimerFcn',@(~,~) timercallback);
    start(t);

    old_data;
    counter;
    
    while true
        data = poll(q, Inf);
        if data == 10
            return
        elseif data ~= old_data
            stop(t);
            old_data = data;
        else

        end
    end

end

